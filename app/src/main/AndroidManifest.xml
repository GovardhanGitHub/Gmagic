<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gmagic">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/gmagic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/gmagic"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AboutPage"></activity>
        <activity android:name=".SuffixNumberSearch" />
        <activity android:name=".PrefixNameSearch" />
        <activity android:name=".AdvancedSearchNumber" />
        <activity android:name=".FindAnonymous" />
        <activity android:name=".SearchName" />
        <activity android:name=".SearchNumber" />
        <activity
            android:name=".Main3Activity"
            android:label="@string/title_activity_main3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoadContacts" />
        <activity android:name=".VerifyPhoneActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>